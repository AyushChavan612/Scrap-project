{
  "version": 2,
  "builds": [
  {
  "src": "index.js",
  "use": "@vercel/node"
  },
  {
  "src": "public/**/*",
  "use": "@vercel/static"
  },
  {
  "src": "views/**/*",
  "use": "@vercel/static"
  },
  {
  "src": "model/**/*",
  "use": "@vercel/static"
  }
  ],
  "routes": [
  {
  "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico))",
  "dest": "/public/$1"
  },
      {
        "src": "/(.*\\.(ejs|js|png|jpg|jpeg|gif|ico))",
        "dest": "/views/$1"
      },
      {
        "src": "/(.*\\.(js|ejs|png|jpg|jpeg|gif|ico))",
        "dest": "/model/$1"
      },
  {
  "src": "/(.*)",
  "dest": "/index.js"
  }
  ]
  }